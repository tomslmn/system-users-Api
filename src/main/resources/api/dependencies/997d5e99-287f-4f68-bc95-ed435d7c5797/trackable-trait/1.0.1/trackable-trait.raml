#%RAML 1.0 Trait
displayName: trackable
headers: 
  x-correlation-id?:
    description: |
          Use the above header to optionally pass a tracking information to this API, for end-to-end traceability of the operation/transaction. 
    maxLength: 50
    type: string
    required: false
    example:
      value: 421cb368-066b-11e1-9647-c5be7398b24e